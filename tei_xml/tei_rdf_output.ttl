<quote/1> a schema:Quotation ;
    rdfs:label "- SIMÓN BOLIVAR" .

<quote/10> a schema:Quotation ;
    rdfs:label "\"Es tierra de infieles\"." ;
    schema:spokenByCharacter <person/Coronel> .

<quote/11> a schema:Quotation ;
    rdfs:label "\"Mucho te amo, pero más te amaré si ahora tienes más juicio que nunca\"" ;
    schema:spokenByCharacter <person/Coronel> .

<quote/2> a schema:Quotation ;
    rdfs:label "\"Vamonós\"" ;
    schema:spokenByCharacter <person/Coronel> .

<quote/3> a schema:Quotation ;
    rdfs:label "\"volando que aquí no nos quiere nadie\"" ;
    schema:spokenByCharacter <person/Coronel> .

<quote/4> a schema:Quotation ;
    rdfs:label "\"Sábado ocho de mayo, día de Santa Juana de Arco\"" ;
    schema:spokenByCharacter <person/mayordomo> .

<quote/5> a schema:Quotation ;
    rdfs:label "\"Está lloviendo desde las tres de la madrugada\"." ;
    schema:spokenByCharacter <person/mayordomo> .

<quote/6> a schema:Quotation ;
    rdfs:label "\"Desde las tres de la madrugada del día en que la asaron los ingleses\"" ;
    schema:spokenByCharacter <person/Coronel> .

<quote/7> a schema:Quotation ;
    rdfs:label "\"No oí los gallos\"." ;
    schema:spokenByCharacter <person/Coronel> .

<quote/8> a schema:Quotation ;
    rdfs:label "\"Aquí no hay gallos\"" ;
    schema:spokenByCharacter <person/mayordomo> .

<quote/9> a schema:Quotation ;
    rdfs:label "\"No hay nada\"" ;
    schema:spokenByCharacter <person/Coronel> .

